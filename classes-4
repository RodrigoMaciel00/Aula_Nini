class ContaBancaria:
    def __init__(self, saldo, titular):
        self.saldo = saldo
        self.titular = titular
    
    def depositar(self, valor):
        self.saldo += valor
    
    def sacar(self, valor):
        if valor <= self.saldo:
            self.saldo -= valor
            print("Saque realizado com sucesso.")
        else:
            print("Saldo insuficiente.")

# Exemplo de uso:
conta = ContaBancaria(1000, "Jo찾o")
conta.depositar(500)
print("Saldo ap처s dep처sito:", conta.saldo)
conta.sacar(200)
print("Saldo ap처s saque:", conta.saldo)